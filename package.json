		"@typescript-eslint/parser": "5.29.0",
		"builtin-modules": "3.3.0",
		"c8": "^10.1.2",
		"esbuild": "0.14.47",
		"lodash": "~4.17.21",
		"obsidian": "latest",
		"tslib": "2.4.0",
		"tsx": "~4.17.0",
		"typescript": "4.7.4"
	}
